public class teste {

	public static void main(String[] args) {
         int [] arr = new int[]{1,2,3,4,5};
		 int a = 5;
		 int index = -1;
		 for(int i=0;i<arr.length;i++)
		{
             if(arr[i] == a)
			{
				 index = i;
				 break;
			 }
		 }
		 if(index ==-1)
			 System.out.println("找不到该数");
		 else
			 System.out.println("找到了 下标是"+index);
	}
}
